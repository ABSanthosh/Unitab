{
  "name": "wxt-svelte-starter",
  "description": "manifest.json description",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:watch": "nodemon --watch src --ext svelte,ts --exec 'wxt'",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "wxt prepare"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^5.0.4",
    "@types/chrome": "^0.1.1",
    "@wxt-dev/module-svelte": "^2.0.3",
    "autoprefixer": "^10.4.21",
    "nodemon": "^3.1.10",
    "sass": "^1.89.2",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.6",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "wxt": "^0.20.6"
  },
  "dependencies": {
    "@lucide/svelte": "^0.526.0"
  }
}
